Map {
  background-color: #b8dee6;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #0d0d0d;
}

#minor_roads {
  line-width:1;
  line-color:#888;
}

#roads {
  line-width:2;
  line-color:#888;
}

#major_roads {
  line-width:4;
  line-color:#888;
}

#buildings {
  line-color:#444;
  line-width:0.5;
  polygon-opacity:1;
  polygon-fill:#444;
}
